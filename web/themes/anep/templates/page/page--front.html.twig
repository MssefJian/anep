{# /** * @file * ANEP's theme implementation to display a single Drupal page. */ #} 
{% set has_breadcrumb = '' %}
<div class="gva-body-wrapper">
   <div class="body-page gva-body-page">
      {% include header_skin %} {% if page.breadcrumbs %} {% set has_breadcrumb = ' has-breadcrumb' %}
      <div class="breadcrumbs">
         {{ page.breadcrumbs }}
      </div>
      {% endif %}
      <div role="main" class="main main-page{{ has_breadcrumb }}">
         <div class="clearfix"></div>
         {% if page.slideshow_content %}
         <div class="slideshow_content area">
            {{ page.slideshow_content }}
         </div>
         {% endif %} {% if page.actualites %}
         <div class="bb-container container">
            <div class="row row-wrapper">
               <div class="gsc-column col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                  <div class="column-inner bg-size-cover">
                     <div class="column-content-inner">
                        <div class="clearfix"></div>
                        <div>
                           <div class="">
                              {{ page.actualites }}
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>
      {% endif %} 
     <div class="ch_bg p-4">
      {% if page.anep_ch %}

      {{ page.anep_ch }}

{% endif %}
     </div>
     
        <div class="clearfix"></div>
          <div class="p-4">
            {% if page.projet %}
              <div class="bb-container full-container">
                <div class="row row-wrapper">
                  <div class="gsc-column col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="column-inner bg-size-cover">
                      <div class="column-content-inner">
                        <div class="clearfix"></div>
                          <div>
                            <div class="">
                              {{ page.projet }}
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            {% endif %}
          </div>
        <div class="clearfix"></div>
   <div id="content" class="content content-full">
      <div class="container container-bg">
         {% include '@anep/page/main.html.twig' %}
      </div>
   </div>
   {% include '@anep/page/footer.html.twig' %}
</div>
{% include '@anep/page/parts/quick-side.html.twig' %}
