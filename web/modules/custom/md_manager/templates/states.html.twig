<div class="col-md-12 map-svg">
  <svg id="Calque_1" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px"
       y="0px" viewBox="0 0 1366 718" style="enable-background:new 0 0 1366 718;" xml:space="preserve" width="100%"
       height="718">

                <g>
                  <g>
                    <g>
                      {% for path in paths %}
                      {{ path|raw }}
                      {% endfor %}
                    </g>
                  </g>
                </g>
              </svg>


  {% for item in data %}
    <div class="reg-content{{ loop.index }} regcontent"  style="display: none;">
      <div class="card">
        <div class="card-body">
          <div class="h4"><span class="badge badge-pill badge-region1">{{ item.title }}</span></div>
          <div>
            {% for subItem in item.subRegion %}
              <li>{{ subItem.title }}</li>
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  {% endfor %}
</div>
